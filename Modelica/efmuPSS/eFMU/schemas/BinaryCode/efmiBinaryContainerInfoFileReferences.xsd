<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../efmiFiles.xsd"/>
	<xs:include schemaLocation="efmiRunTimeComplianceInformation.xsd"/>
	<xs:element name="BinaryContainerInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RunTimeComplianceInformation" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="FileReference" minOccurs="0" maxOccurs="unbounded"/>
				<!-- Used kinds may be: MapFile, CalibrationInformationFile, MeasurementInformationFile, DiagnosisInformationFile, ValidationAndVerificationFile, ConfigurationFile -->
				<!-- MapFile: File reference to a map file generated by a linker. -->
				<!-- CalibrationInformationFile: -->
				<!-- MeasurementInformationFile: File reference to e.g. equivalence class information xml file within the model representation. -->
				<!-- DiagnosisInformationFile: -->
				<!-- ValidationAndVerificationFile: File reference to e.g. equivalence class information xml file within the model representation.-->
				<!-- ConfigurationFile: File reference to e.g. configuration files to define the behavior of a communication stack.-->
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
